      "post": {
        "description": "Register a new user",
        "consumes": ["multipart/form-data"],
        "parameters": [
          {
            "name": "userName",
            "in": "formData",
            "type": "string",
            "required": true,
            "description": "User's name"
          },
          {
            "name": "email",
            "in": "formData",
            "type": "string",
            "required": true,
            "description": "User's email"
          },
          {
            "name": "address",
            "in": "formData",
            "type": "string",
            "required": true,
            "description": "User's address"
          },
          {
            "name": "phoneNumber",
            "in": "formData",
            "type": "string",
            "required": true,
            "description": "User's phone number"
          },
          {
            "name": "password",
            "in": "formData",
            "type": "string",
            "required": true,
            "description": "User's password"
          },
          {
            "name": "coverImage",
            "in": "formData",
            "type": "file",
            "required": true,
            "description": "Required cover image"
          }
        ],
        "responses": {
          "201": {
            "description": "User registered successfully"
          }
        }
      }
    },
